      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Projects\CP06\Com.RePower.Ocv.Project.Cp06.Ocv0\Com.RePower.Ocv.Project.Cp06.Ocv0.csproj" />
