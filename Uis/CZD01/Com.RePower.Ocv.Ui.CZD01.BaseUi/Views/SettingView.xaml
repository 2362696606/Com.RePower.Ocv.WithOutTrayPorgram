<Window
    x:Class="Com.RePower.Ocv.Ui.CZD01.BaseUi.Views.SettingView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:Com.RePower.Ocv.Ui.CZD01.BaseUi.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:uiBaseViews="clr-namespace:Com.RePower.Ocv.Ui.UiBase.Views;assembly=Com.RePower.Ocv.Ui.UiBase"
    Title="SettingView"
    Width="800"
    Height="450"
    mc:Ignorable="d">
    <TabControl>
        <TabItem Header="电池参数">
            <uiBaseViews:SettingView SettingObj="{Binding SettingManager.CurrentBatteryStandard}" />
        </TabItem>
        <TabItem Header="测试选项">
            <uiBaseViews:SettingView SettingObj="{Binding SettingManager.CurrentTestOption}" />
        </TabItem>
    </TabControl>
</Window>
