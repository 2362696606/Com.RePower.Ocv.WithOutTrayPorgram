			<section name="Com.RePower.Ocv.Project.Byd.CB09.Settings.NetDmmSetting" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
			<section name="Com.RePower.Ocv.Project.Byd.CB09.Settings.SerialPortOhmSetting" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
			<section name="Com.RePower.Ocv.Project.Byd.CB09.Settings.SerialPortSwitchBoardSetting" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
			<section name="Com.RePower.Ocv.Project.Byd.CB09.Settings.NetTemperatureSensorSetting" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
